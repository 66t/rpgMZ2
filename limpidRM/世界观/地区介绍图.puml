@startuml
'https://plantuml.com/object-diagram

map 绿洲 {
熔炉 => 
地下 => 
冰层 => 
海洋 => 
云层 => 
太空 => 
}
map 熔炉 {
扭曲 =>
腐蚀 =>
焚铄 =>
熔炉星礁 =>
}

map 地下 {
迷雾峡谷=>
枢纽=>
彼岸=>
家园=>
堕落之喉=>
风暴峭壁=>
幽潭湿地=>
外隅=>
灰穴=>
新世界=>
}
map 冰层 {
永昼冰荒=>
迷雾之源=>
火舌之沼=>
磁晶星礁=>
}
map 海洋 {
潜渊星礁=>
}
map 云层 {
天际星礁=>
}
map 太空 {
异界星礁=>
}

map 熔炉星礁{
熔炉星核=>
}
map 磁晶星礁{
电晶森林=>
始源之泉=>
磁晶星核=>
}
map 潜渊星礁{
